{
    "type": "object",
    "properties": {
        "id": { "type": "string", "format": "uuid" },
        "day": { "type": "integer", "enum": [
            0, 1, 2, 3, 4, 5, 6
        ]},
        "start_hour": {
            "type": "string",
            "pattern": "\\d{2}:\\d{2}",
            "example": "08:00"
        },
        "end_hour": {
            "type": "string",
            "pattern": "\\d{2}:\\d{2}",
            "example": "20:00"
        },
    }
}
